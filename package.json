{
  "name": "vscode-go2dot-view",
  "displayName": "vscode-go2dot-view",
  "description": "Generate graphical view of a golang package",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "GO View Package",
      "type": "object",
      "properties": {
        "goViewPkg.showPrivate": {
          "type": "boolean",
          "default": true,
          "description": "Show private variables and functions."
        },
        "goViewPkg.dotCmd": {
          "type": "string",
          "default": "dot",
          "description": "dot executable"
        },
        "goViewPkg.extraOptions": {
          "type": "string",
          "default": "",
          "description": "Extra options for dot"
        },
        "goViewPkg.format": {
          "type": "string",
          "default": "png",
          "description": "Image format"
        },
        "goViewPkg.go2DotDir": {
          "type": "string",
          "default": "${HOME}/.config/vscode-go2dot-view/go2dot",
          "description": "Image format"
        }
      }
    },
    "commands": [
      {
        "command": "vscode-go2dot-view.view-package",
        "title": "GO: view package"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}